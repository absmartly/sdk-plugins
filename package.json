{
  "name": "@absmartly/sdk-plugins",
  "version": "1.0.1",
  "description": "Collection of plugins for ABsmartly JavaScript SDK",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/absmartly-sdk-plugins.min.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "es",
    "dist",
    "types"
  ],
  "scripts": {
    "clean": "rm -rf lib es dist types",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint src --ext .ts,.tsx",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "compile:es": "tsc -p tsconfig.es.json",
    "compile:types": "tsc -p tsconfig.types.json",
    "compile": "npm run compile:cjs && npm run compile:es && npm run compile:types",
    "bundle": "webpack --mode production --env BUILD_TYPE=all",
    "bundle:dom": "webpack --mode production --env BUILD_TYPE=dom-only",
    "bundle:lite": "webpack --mode production --env BUILD_TYPE=dom-lite",
    "bundle:full": "webpack --mode production --env BUILD_TYPE=dom-full",
    "bundle:cookie": "webpack --mode production --env BUILD_TYPE=cookie-only",
    "bundle:vitals": "webpack --mode production --env BUILD_TYPE=vitals-only",
    "postbundle": "cp dist/*.min.js ../absmartly.com/static/ 2>/dev/null || true",
    "bundle:dev": "webpack --mode development --env BUILD_TYPE=all --devtool source-map",
    "bundle:dev:all": "npm run bundle:dev:dom && npm run bundle:dev:lite && npm run bundle:dev:full && npm run bundle:dev:cookie && npm run bundle:dev:vitals && npm run bundle:dev:overrides",
    "bundle:dev:dom": "webpack --mode development --env BUILD_TYPE=dom-only --devtool source-map",
    "bundle:dev:lite": "webpack --mode development --env BUILD_TYPE=dom-lite --devtool source-map",
    "bundle:dev:full": "webpack --mode development --env BUILD_TYPE=dom-full --devtool source-map",
    "bundle:dev:cookie": "webpack --mode development --env BUILD_TYPE=cookie-only --devtool source-map",
    "bundle:dev:vitals": "webpack --mode development --env BUILD_TYPE=vitals-only --devtool source-map",
    "bundle:dev:overrides": "webpack --mode development --env BUILD_TYPE=overrides-lite --devtool source-map && webpack --mode development --env BUILD_TYPE=overrides-full --devtool source-map",
    "build": "npm run clean && npm run format && npm run lint && npm run compile && npm run bundle",
    "build:dev": "npm run clean && npm run format && npm run lint && npm run compile && npm run bundle:dev",
    "build:all": "npm run build && npm run bundle:dev && npm run rename:dev",
    "rename:dev": "cd dist && for file in *.js; do if [[ ! \"$file\" == *.min.js ]] && [[ ! \"$file\" == *.dev.js ]]; then mv \"$file\" \"${file%.js}.dev.js\"; fi; done; exit 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "tsc --watch",
    "dev:all": "npm run compile:cjs -- --watch & npm run compile:es -- --watch & npm run compile:types -- --watch",
    "watch": "webpack --mode development --watch --env BUILD_TYPE=all --devtool source-map",
    "prepublishOnly": "npm run build && npm test",
    "deploy": "npm run build && npm test && npm publish --access public",
    "deploy:dry-run": "npm run build && npm test && npm publish --dry-run --access public",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/absmartly/sdk-plugins.git"
  },
  "keywords": [
    "absmartly",
    "ab-testing",
    "experimentation",
    "dom-manipulation",
    "visual-editor"
  ],
  "author": "ABsmartly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/absmartly/dom-changes-plugin/issues"
  },
  "homepage": "https://github.com/absmartly/dom-changes-plugin#readme",
  "peerDependencies": {
    "@absmartly/javascript-sdk": "^1.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "web-vitals": "^5.1.0"
  }
}
